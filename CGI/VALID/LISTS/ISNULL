#!/bin/bash
#

########################################
#	ISNULL.CGI
#
#       $1:検証する文字列や数値
#       
#	Written by hiramatsu


#######################################
LANG=ja_JP.UTF-8

#Validation
#OKの場合、OK|OKメッセージ、
#NGの場合、NG|NGメッセージを返す
[ -n "$1" ] && echo "OK|Is_Not_Null." || echo "NG|Is_Null." 

#######################################
#Return cookie words
exit 0

