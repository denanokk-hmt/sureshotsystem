mode=$(nameread MODE $tmp-name)
++ nameread MODE /tmp/tmp_5419-name
+ mode=CHK
[ "$mode" == "" ] && mode="INIT"
+ '[' CHK == '' ']'
[ "$mode" == "_" ] && mode="INIT"
+ '[' CHK == _ ']'
[ "$(cat $tmp-NG)" == "NG" ] && mode="NG"
++ cat /tmp/tmp_5419-NG
